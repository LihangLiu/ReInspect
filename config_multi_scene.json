{
    "data": {
        "reinspect_train_idl": "./data/brainwash/brainwash_train.idl",
        "reinspect_test_idl": "./data/brainwash/brainwash_test.idl",
        "boost_test_gen": "./multi_scene_data/annnotation/test.idl",
        "multi_scene_imglist_txt": "./multi_scene_data/pre_data/image_list_txt.txt",
        "idl_mean": "./data/brainwash_mean.npy",
        "lstm_input_mean": "./data/lstm_input_mean.txt"
    },
    "solver": {
        "base_lr": 0.005,
        "max_iter": 10000,
        "test_iter": 100,
        "test_interval": 200,
        "boost_interval": 200,
        "momentum": 0.5,
        "random_seed": 1,
        "clip_gradients": 0.1,
        "gamma": 0.8,
        "stepsize": 100000,
        "weights": ""
    },
    "logging": {
        "display_interval": 100,
        "snapshot_prefix": "./tmp/boost_cluster",
        "snapshot_interval": 200,
        "schematic_path": "./tmp/boost.png",
        "graph_prefix": "./tmp/history",
        "log_file": "./tmp/log_boost.txt",
        "graph_interval": 500
    },
    "net": {
        "init_range": 0.1,
        "max_len": 5,
        "lstm_num_cells": 250,
        "dropout_ratio": 0.15,
        "hungarian_match_ratio": 0.5,
        "hungarian_permute_matches": true,
        "hungarian_loss_weight": 0.03,
        "googlenet_lr_mult": 4,
        "grid_height": 15,
        "grid_width": 20,
        "img_height": 480,
        "img_width": 640,
        "region_size": 64,
        "ignore_label": -1
    }
}
